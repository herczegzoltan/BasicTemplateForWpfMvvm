<Window x:Class="ApplicationWPFProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ApplicationWPFProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="250">
    <Grid>
        <Button Content="Button" HorizontalAlignment="Left" Margin="77,59,0,0" VerticalAlignment="Top" Width="75" Command="{Binding CmdSend}" />
        <TextBox Width="250" Height="25" Text="{Binding CustomText}"/>
    </Grid>
</Window>
